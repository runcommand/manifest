{
    "name": "runcommand/manifest",
    "description": "See what's going on inside of WordPress.",
    "type": "wp-cli-package",
    "homepage": "https://runcommand.io/wp/manifest/",
    "license": "MIT",
    "authors": [],
    "minimum-stability": "dev",
    "autoload": {
        "files": [ "command.php" ]
    },
    "require-dev": {
        "behat/behat": "~2.5"
